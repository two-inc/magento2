<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
        <policies>
            <policy id="form-action">
                <values>
                    <value id="two" type="host">two.inc</value>
                    <value id="www_two" type="host">www.two.inc</value>
                    <value id="other_two" type="host">*.two.inc</value>
                    <value id="sandbox_api" type="host">api.sandbox.two.inc</value>
                    <value id="www_sandbox_api" type="host">www.api.sandbox.two.inc</value>
                </values>
            </policy>
            <policy id="base-uri">
                <values>
                    <value id="two" type="host">two.inc</value>
                    <value id="www_two" type="host">www.two.inc</value>
                    <value id="other_two" type="host">*.two.inc</value>
                    <value id="sandbox_api" type="host">api.sandbox.two.inc</value>
                    <value id="www_sandbox_api" type="host">api.sandbox.two.inc</value>
                </values>
            </policy>
        </policies>
</csp_whitelist>
